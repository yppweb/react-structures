{
  "name": "react-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-loader": "^8.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "start": "webpack-dev-server --config ./build/webpack.base.js --open --progress",
    "build:dev": "cross-env NODE_ENV=development webpack  --config ./build/webpack.dev.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack  --config ./build/webpack.prod.config.js",
    "lint": "eslint --ext .js src",
    "fix": "eslint --fix",
    "test": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo 'husky' && npm run test"
    }
  },
  "eslintConfig": {
    "extends": []
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^2.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.0.0",
    "husky": "^7.0.4",
    "style-loader": "^2.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.43",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  }
}
